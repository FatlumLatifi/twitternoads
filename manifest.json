{
    "manifest_version": 2,
    "name": "twitter no ads",
    "version": "1.0",
    "description": "Appending a line of CSS to set the display of the ads to none. ",
    "permissions": [
     "https://x.com/*"
    ],
    "content_scripts": [
     {
      "matches": ["https://x.com/*"],
      "css": ["some.css"],
      "js": ["some.js"]
     }
    ]
   }